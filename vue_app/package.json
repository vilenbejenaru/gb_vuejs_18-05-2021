    "vue-router": "^3.5.1",
    "vuetify": "^2.5.5",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.5",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-service": "^3.3.1",
    "@vue/test-utils": "^1.2.0",
